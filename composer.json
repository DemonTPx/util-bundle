{
    "name": "demontpx/util-bundle",
    "description": "Various Symfony2 utils",
    "type": "symfony-bundle",
    "keywords": ["utility"],
    "license": "MIT",
    "require": {
        "php": ">=5.5",
        "symfony/framework-bundle": "~2.4",
        "symfony/form": "~2.4",
        "doctrine/orm": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "twig/twig": "~1.16",
        "cocur/slugify": "^0.4"
    },
    "autoload": {
        "psr-4": { "Demontpx\\UtilBundle\\": "src/" }
    },
    "config": {
        "bin-dir": "bin"
    }
}
