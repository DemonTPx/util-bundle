{
    "name": "demontpx/util-bundle",
    "description": "Various Symfony utils",
    "type": "symfony-bundle",
    "keywords": ["utility"],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-intl": "*",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/form": "^4.4|^5.0",
        "doctrine/orm": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "symfony/yaml": "^4.4|^5.0",
        "symfony/browser-kit": "^4.4|^5.0",
        "symfony/dom-crawler": "^4.4|^5.0",
        "twig/twig": "^2.4|^3.0",
        "cocur/slugify": "^3.2"
    },
    "conflict": {
        "phpunit/phpunit": "<8.0"
    },
    "autoload": {
        "psr-4": { "Demontpx\\UtilBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Demontpx\\UtilBundle\\": "tests/" }
    },
    "config": {
        "bin-dir": "bin"
    }
}
