{
    "name": "demontpx/util-bundle",
    "description": "Various Symfony utils",
    "type": "symfony-bundle",
    "keywords": ["utility"],
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "symfony/framework-bundle": "^4.0",
        "symfony/form": "^4.0",
        "doctrine/orm": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/yaml": "^4.0",
        "symfony/browser-kit": "^4.0",
        "symfony/dom-crawler": "^4.0",
        "twig/twig": "^2.4",
        "cocur/slugify": "^3.1"
    },
    "autoload": {
        "psr-4": { "Demontpx\\UtilBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Demontpx\\UtilBundle\\": "tests/" }
    },
    "config": {
        "bin-dir": "bin"
    }
}
